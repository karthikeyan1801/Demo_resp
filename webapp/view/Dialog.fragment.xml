<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Products Found"
		title="Select Airline data"
		search="handleSearch"
		confirm="handleClose"
		cancel="handleClose"
		items="{
			path: '/scarEntitySet'
		}" >
		<StandardListItem
			title="{Carrname}"
			description="{Carrid}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>